{
    "redirects": [
      {
        "source": "/compare",
        "destination": "/compare.html",
        "permanent": true
      }
    ],
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  

