{
    "redirects": [
        {
            "source": "/compare",
            "destination": "/compare.html",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "functions": {
        "api/openai.js": {
            "runtime": "vercel/node@18.0.0"
        }
    },
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ]
}

